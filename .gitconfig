[user]
  name = Osamu Takiya
  email = takiya@toran.sakura.ne.jp
[diff]
  tool = vimdiff
[difftool]
  prompt = false
[merge]
  tool = vimdiff
  conflictstyle = diff3
  prompt = false
[push]
  default = current
[core]
  autocrlf = input
  quotepath = false
[fetch]
  prune = true
[pager]
  branch = cat
  config = cat
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
